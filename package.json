{
  "name": "sass-lint-underdog",
  "version": "1.2.0",
  "description": "Wrapper for sass-lint with proper defaults for Underdog.io",
  "main": "index.js",
  "bin": {
    "sass-lint-underdog": "bin/sass-lint-underdog"
  },
  "scripts": {
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "eslint index.js bin/sass-lint-underdog",
    "lint-sass": "bin/sass-lint-underdog './test/*.scss'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/underdogio/sass-lint-underdog.git"
  },
  "keywords": [
    "sass",
    "lint",
    "sass-lint",
    "config",
    "styleguide",
    "underdog"
  ],
  "author": "Brett Langdon <me@brett.is>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/underdogio/sass-lint-underdog/issues"
  },
  "homepage": "https://github.com/underdogio/sass-lint-underdog",
  "dependencies": {
    "optimist": "0.6.1",
    "sass-lint": "git://github.com/underdogio/sass-lint#070df8e"
  },
  "devDependencies": {
    "eslint": "2.2.0",
    "eslint-config-underdog": "1.0.0"
  }
}
